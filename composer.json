{
	"name": "modette/core",
	"description": "Core of Modette systems. Your best friend.",
	"type": "library",
	"license": "MPL-2.0",
	"keywords": [
		"contributte",
		"core",
		"cache",
		"console",
		"di",
		"debugger",
		"events",
		"filesystem",
		"logs",
		"nette"
	],
	"homepage": "https://github.com/modette/cmf",
	"authors": [
		{
			"name": "Marek Barto≈°",
			"homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": "^7.2.0",
		"contributte/cache": "^0.5.0",
		"contributte/console": "^0.7.0",
		"contributte/console-extra": "^0.5.0",
		"contributte/di": "^0.4.0",
		"contributte/event-dispatcher": "^0.6.0",
		"contributte/flysystem": "^0.2.0",
		"contributte/model": "^0.2.0",
		"contributte/monolog": "^0.4.0",
		"contributte/tracy": "^0.5.0",
		"marc-mabe/php-enum": "^4.0.0",
		"modette/exceptions": "^1.0.0",
		"modette/module-installer": "^1.0.0",
		"nette/neon": "~3.0.0",
		"ramsey/uuid": "^3.8.0"
	},
	"require-dev": {
		"composer/composer": "^1.7.0",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^0.11.0",
		"phpstan/phpstan-nette": "^0.11.0",
		"phpstan/phpstan-strict-rules": "^0.11.0",
		"phpunit/phpunit": "^8.2.3"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Modette\\Core\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Modette\\Core\\": "tests"
		}
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
